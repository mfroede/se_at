<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.portTable{
			margin-top:3em;
			margin-left:3em;
		}
		.portLine{
			margin-right:3em;
			font-size: :2em;
		}
	</ui:style>
	<g:FlowPanel>
	<!-- InteractionElements -->
	<!-- Start of user code Portfolio 
	     Start protectetRegion -->
				<g:Grid styleName="{style.portTable}" ui:field="lebenslauf">
						<g:row>
						    <g:customCell>
								<g:Label styleName="{style.portLine}" text="Name"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Hans-Peter Mueller"/>
							</g:customCell>
						</g:row>
						<g:row>
						    <g:customCell>
								<g:Label styleName="{style.portLine}" text="Adresse"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Musterweg 5, 15236 Stadt"/>			
							</g:customCell>
						</g:row>
						<g:row>
						    <g:customCell>
								<g:Label styleName="{style.portLine}" text="Familienstand"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Ledig"/>			
							</g:customCell>
						</g:row>
						<g:row>
						    <g:customCell>
								<g:Label styleName="{style.portLine}" text="Job"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="nein habe ich nicht"/>			
							</g:customCell>
